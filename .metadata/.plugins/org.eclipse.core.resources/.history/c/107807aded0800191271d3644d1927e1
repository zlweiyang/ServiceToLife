<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <!-- namespace是dao中的接口 -->
<mapper namespace="com.zl.o2o.dao.AreaDao">
    <!-- id是对应方法名 -->
	<select id="queryArea" resultType="com.imooc.o2o.entity.Area">
		SELECT
		area_id,
		area_name,
		priority,
		create_time,
		last_edit_time
		FROM
		tb_area
		ORDER BY
		priority DESC
	</select>

</mapper>